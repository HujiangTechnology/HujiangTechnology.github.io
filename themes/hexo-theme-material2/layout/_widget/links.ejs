<% if (theme.links.length){ %>
<div class="widget">
	<h4><%= __('links') %></h4>
	<ul class="blogroll list-unstyled">
	<% for (var i in theme.links){ %>
		<li> <a href="<%- theme.links[i].url %>" title="<%- theme.links[i].intro %>" target="_blank"> <i class="mdi-action-launch"></i> <%= theme.links[i].title %></a></li>
	<% } %>
	</ul>
</div>
<% } %>
